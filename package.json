{
  "name": "jaipasdevoiture",
  "version": "0.0.0",
  "description": "Site vitrine pour le loueur de voiture",
  "main": "index.html",
  "scripts": {
    "dev": "npm run build-CSS -- --watch",
    "build": "npm run build-CSS && npm run post-CSS",
    "build-CSS": "npm run build-CSS:yasmina && npm run build-CSS:mariana",
    "build-CSS:yasmina": "node-sass ./yasmina/style.scss ./yasmina/style.css",
    "build-CSS:mariana": "node-sass ./mariana/style.scss ./mariana/style.css",
    "post-CSS": "postcss --output ./mariana/style.css ./mariana/style.css && postcss --output ./yasmina/style.css ./yasmina/style.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GMartigny/jaipasdevoiture.git"
  },
  "author": "GMartigny",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/GMartigny/jaipasdevoiture/issues"
  },
  "homepage": "https://github.com/GMartigny/jaipasdevoiture#readme",
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^3.0.0",
    "bulma": "^0.9.1",
    "cssnano": "^4.1.10",
    "node-sass": "^4.14.1",
    "postcss": "^8.1.1",
    "postcss-cli": "^8.1.0"
  }
}
